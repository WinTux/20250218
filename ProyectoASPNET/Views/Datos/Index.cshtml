<h3>Ejemplos con Datos simples</h3>

Edad: @ViewBag.edad
<br>
Nombre: @ViewBag.nombre
<br>
Es casado: @ViewBag.casado
<br>
Estatura: @ViewBag.estatura
<br>
Fecha de nacimiento: @ViewBag.fechaNacimiento
<br>

<h3>Ejemplos mostrando un producto</h3>
<table class="table">
    <tr>
        <td>ID</td>
        <td>@ViewBag.producto.Id</td>
    </tr>
    <tr>
        <td>Nombre</td>
        <td>@ViewBag.producto.Nombre</td>
    </tr>
    <tr>
        <td>Foto</td>
        <td><img src="~/imagenes/@ViewBag.producto.Foto" width="100"></td>
    </tr>
    <tr>
        <td>Precio</td>
        <td>@ViewBag.producto.Precio</td>
    </tr>
    <tr>
        <td>Cantidad</td>
        <td>@ViewBag.producto.Cantidad</td>
    </tr>
    <tr>
        <td>Total</td>
        <td>@(ViewBag.producto.Precio * ViewBag.producto.Cantidad)</td>
    </tr>
</table>

<h3>Ejemplos mostrando una lista de productos</h3>

<table class="table table-info">
    <tr>
        <td>ID</td>
        <td>Nombre</td>
        <td>Foto</td>
        <td>Precio</td>
        <td>Cantidad</td>
        <td>Total</td>
    </tr>
    @foreach (var p in ViewBag.productos)
    
    {
        <tr>
            <td>@p.Id</td>
            <td>@p.Nombre</td>
            <td><img src="~/imagenes/@p.Foto" height="100"></td>
            <td>@p.Precio</td>
            <td>@p.Cantidad</td>
            <td>@(p.Precio * p.Cantidad)</td>
        </tr>
        
    }
</table>