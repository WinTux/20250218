<table class="table">
    <tr>
        <td>ID</td>
        <td>@ViewBag.cuenta.Id</td>
    </tr>
    <tr>
        <td>Usuario</td>
        <td>@ViewBag.cuenta.Usuario</td>
    </tr>
    <tr>
        <td>Password</td>
        <td>@ViewBag.cuenta.Password</td>
    </tr>
    <tr>
        <td>Descripción</td>
        <td>@ViewBag.cuenta.Descripcion</td>
    </tr>
    <tr>
        <td>Disponible</td>
        <td>@ViewBag.cuenta.Disponible</td>
    </tr>
    <tr>
        <td>Género</td>
        <td>@ViewBag.cuenta.Genero</td>
    </tr>
    <tr>
        <td>Lenguajes</td>
        <td>
            @foreach (var leng in ViewBag.cuenta.Lenguajes) { 
                <span>@leng</span><br>
            }
        </td>
    </tr>
    <tr>
        <td>Cargo</td>
        <td>@ViewBag.cuenta.Cargo</td>
    </tr>
</table>